		function pitchingParameters(){
			var qs;
			qs="";
			qs = qs + "P=ackld001";
			qs = qs + "&M=" + document.getElementById('pitching_month').value;
			qs = qs + "&Hand=" + document.getElementById('pitching_hand').value;
			qs = qs + "&HA=" + document.getElementById('pitching_home').value;
			qs = qs + "&T=" + document.getElementById('pitching_team').value;
			qs = qs + "&Ln=" + document.getElementById('pitching_lineup').value;
			qs = qs + "&Pos=" + document.getElementById('pitching_position').value;
			qs = qs + "&DN=" + document.getElementById('pitching_daynight').value;
			qs = qs + "&Park=" + document.getElementById('pitching_stadium').value;
			qs = qs + "&Outs=" + document.getElementById('pitching_outs').value;
			qs = qs + "&Leadoff=" + document.getElementById('pitching_leadoff').checked;
			qs = qs + "&Sit=" + document.getElementById('pitching_situation').value;
			qs = qs + "&Last=" + document.getElementById('pitching_last').checked;
			qs = qs + "&Swing1=" + document.getElementById('pitching_swing1').value;
			qs = qs + "&Count=" + document.getElementById('pitching_pitchCount').value;
			qs = qs + "&PType=" + document.getElementById('pitching_pitcherType').value;
			qs = qs + "&FP=" + document.getElementById('pitching_pitch1').value;
			qs = qs + "&Batter=" + document.getElementById('pitching_batterID').value;
			qs = qs + "&Weekday=" + document.getElementById('pitching_day').value; 
			qs = qs + "&Inning=" + document.getElementById('pitching_inning').value; 
			qs = qs + "&RIM=" + document.getElementById('pitching_rim').checked;
	//alert(qs);		
			showAjax('include/reports_splits_pitching.asp?' + qs,'pitchingReports','');
	
		}
		
		
		
		
		function showMLBOnlyP(){
			
			resetFiltersP();
			
			document.getElementById('pitching_mlb_only').style.backgroundColor='gold';
			for (i=1;i<200;i++) {
				if (document.getElementById("nonp" + i)){
					document.getElementById("nonp" + i).style.display='none';
				} else if (document.getElementById("ncaap" + i)){
					document.getElementById("ncaap" + i).style.display='none';
				}  else {
				
					//update year value
					if (document.getElementById("yearp" + i)) {
						document.getElementById("yearp" + i).style.color='#000000';
					} else if (document.getElementById("yearp" + i + "hide")) {
						document.getElementById("yearp" + i + "hide").style.color='#000000';
					}
				}
			}	

		}

		
		function showMinorsOnlyP(){
			
			resetFiltersP();
			
			document.getElementById('pitching_minor_only').style.backgroundColor='gold';
			for (i=1;i<200;i++) {
				if (document.getElementById("mlbp" + i)){
					document.getElementById("mlbp" + i).style.display='none';
				} else if (document.getElementById("ncaap" + i)){
					document.getElementById("ncaap" + i).style.display='none';
				} else {
				
					//update year value
					if (document.getElementById("yearp" + i)) {
						document.getElementById("yearp" + i).style.color='#000000';
					} else if (document.getElementById("yearp" + i + "hide")) {
						document.getElementById("yearp" + i + "hide").style.color='#000000';
					}
				}
			}		
		}		
		
		function show_ALLP(){
			
			resetFiltersP();
			
			document.getElementById('all_stats_p').style.backgroundColor='gold';
			document.getElementById('pitching_mlb_only').style.backgroundColor='white';	
			document.getElementById('pitching_minor_only').style.backgroundColor='white';	
		}
		
		function resetFiltersP(){
			document.getElementById('all_stats_p').style.backgroundColor='white';
			document.getElementById('pitching_mlb_only').style.backgroundColor='white';	
			document.getElementById('pitching_minor_only').style.backgroundColor='white';	
			
			for (i=1;i<200;i++) {
				if (document.getElementById("nonp" + i)){
					document.getElementById("nonp" + i).style.display='table-row';
				} else if (document.getElementById("mlbp" + i)){
					document.getElementById("mlbp" + i).style.display='table-row';
				} else if (document.getElementById("ncaap" + i)){
					document.getElementById("ncaap" + i).style.display='table-row';
				}		
							
				//Reset YEars
				if (document.getElementById("yearp" + i)) {
					document.getElementById("yearp" + i).style.color='black';
				} else if (document.getElementById("yearp" + i + "hide")) {
					 if (document.getElementById("mlbp" + i)){
							document.getElementById("yearp" + i + "hide").style.color='#eeeeee';
					 } else {
					 		document.getElementById("yearp" + i + "hide").style.color='white';
					}
				}
				
			}
		}
					
</script>

