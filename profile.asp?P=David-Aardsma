		function battingParameters(){
			var qs;
			qs="";
			qs = qs + "P=aardd001";
			qs = qs + "&M=" + document.getElementById('batting_month').value;
			qs = qs + "&Hand=" + document.getElementById('batting_hand').value;
			qs = qs + "&HA=" + document.getElementById('batting_home').value;
			qs = qs + "&T=" + document.getElementById('batting_team').value;
			qs = qs + "&Ln=" + document.getElementById('batting_lineup').value;
			qs = qs + "&Pos=" + document.getElementById('batting_position').value;
			qs = qs + "&DN=" + document.getElementById('batting_daynight').value;
			qs = qs + "&Park=" + document.getElementById('batting_stadium').value;
			qs = qs + "&Outs=" + document.getElementById('batting_outs').value;
			qs = qs + "&Leadoff=" + document.getElementById('batting_leadoff').checked;
			qs = qs + "&Sit=" + document.getElementById('batting_situation').value;
			qs = qs + "&Last=" + document.getElementById('batting_last').checked;
			qs = qs + "&Swing1=" + document.getElementById('batting_swing1').value;
			qs = qs + "&Count=" + document.getElementById('batting_pitchCount').value;
			qs = qs + "&PType=" + document.getElementById('batting_pitcherType').value;
			qs = qs + "&FP=" + document.getElementById('batting_pitch1').value;
			qs = qs + "&Pitcher=" + document.getElementById('batting_pitcherID').value;
			qs = qs + "&Weekday=" + document.getElementById('batting_day').value; 
			qs = qs + "&Inning=" + document.getElementById('batting_inning').value; 
			qs = qs + "&RIM=" + document.getElementById('batting_rim').checked;
	//alert(qs);		
			showAjax('include/reports_splits_batting.asp?' + qs,'battingReports','');
	
		}
		
		

		
		function showMLBOnly(){
			
			resetFilters();
			
			document.getElementById('batting_mlb_only').style.backgroundColor='gold';
			for (i=1;i<200;i++) {
				if (document.getElementById("non" + i)){
					document.getElementById("non" + i).style.display='none';
				} else if (document.getElementById("ncaa" + i)){
					document.getElementById("ncaa" + i).style.display='none';
				}  else {
				
					//update year value
					if (document.getElementById("year" + i)) {
						document.getElementById("year" + i).style.color='#000000';
					} else if (document.getElementById("year" + i + "hide")) {
						document.getElementById("year" + i + "hide").style.color='#000000';
					}
				}
			}	

		}

		
		function showMinorsOnly(){
			
			resetFilters();
			
			document.getElementById('batting_minor_only').style.backgroundColor='gold';
			for (i=1;i<200;i++) {
				if (document.getElementById("mlb" + i)){
					document.getElementById("mlb" + i).style.display='none';
				} else if (document.getElementById("ncaa" + i)){
					document.getElementById("ncaa" + i).style.display='none';
				} else {
				
					//update year value
					if (document.getElementById("year" + i)) {
						document.getElementById("year" + i).style.color='#000000';
					} else if (document.getElementById("year" + i + "hide")) {
						document.getElementById("year" + i + "hide").style.color='#000000';
					}
				}
			}		
		}		
		
		function show_ALL(){
			
			resetFilters();
			
			document.getElementById('all_stats').style.backgroundColor='gold';
			document.getElementById('batting_mlb_only').style.backgroundColor='white';	
			document.getElementById('batting_minor_only').style.backgroundColor='white';	
		}
		
		function resetFilters(){
			document.getElementById('all_stats').style.backgroundColor='white';
			document.getElementById('batting_mlb_only').style.backgroundColor='white';	
			document.getElementById('batting_minor_only').style.backgroundColor='white';	
			
			for (i=1;i<200;i++) {
				if (document.getElementById("non" + i)){
					document.getElementById("non" + i).style.display='table-row';
				} else if (document.getElementById("mlb" + i)){
					document.getElementById("mlb" + i).style.display='table-row';
				} else if (document.getElementById("ncaa" + i)){
					document.getElementById("ncaa" + i).style.display='table-row';
				}		
							
				//Reset YEars
				if (document.getElementById("year" + i)) {
					document.getElementById("year" + i).style.color='black';
				} else if (document.getElementById("year" + i + "hide")) {
					 if (document.getElementById("mlb" + i)){
							document.getElementById("year" + i + "hide").style.color='#eeeeee';
					 } else {
					 		document.getElementById("year" + i + "hide").style.color='white';
					}
				}
				
			}
		}
		
</script>
