<script>
		function showPlayerSearch(obj){
			
			var str =  document.getElementById(obj).value;


			
			if (str.length > 2){
				//document.getElementById(obj).style.display='inline';
				
				showAjax('../reports/xPlayers.asp?W=P2&Q=' + str,'playerList1','')
			}
		}	
</script>

